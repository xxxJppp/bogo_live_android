<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TCLayerOperationView">
        <attr name="src" format="reference" />            <!-- 用于缩放旋转的图标 -->
        <attr name="editable" format="boolean" />          <!-- 是否处于可编辑状态 -->
        <attr name="frameColor" format="color" />         <!-- 边框颜色 -->
        <attr name="frameWidth" format="dimension" />     <!-- 边框线宽度 -->
        <attr name="framePadding" format="dimension" />   <!-- 边框与图片的间距 -->
        <attr name="degree" format="float" />             <!-- 旋转角度 -->
        <attr name="scale" format="float" />              <!-- 缩放比例 -->
        <attr name="controlDrawable" format="reference" /> <!-- 控制图标 -->
        <attr name="editDrawable" format="reference" />   <!--编辑图标位置-->
        <attr name="deleteDrawable" format="reference" /> <!--删除图标位置-->
        <attr name="controlLocation">                     <!-- 控制图标的位置 -->
            <enum name="left_top" value="0" />
            <enum name="right_top" value="1" />
            <enum name="right_bottom" value="2" />
            <enum name="left_bottom" value="3" />
        </attr>
        <attr name="editLocation">                     <!-- 编辑图标的位置 -->
            <enum name="left_top" value="0" />
            <enum name="right_top" value="1" />
            <enum name="right_bottom" value="2" />
            <enum name="left_bottom" value="3" />
        </attr>
        <attr name="deleteLocation">                     <!-- 删除图标的位置 -->
            <enum name="left_top" value="0" />
            <enum name="right_top" value="1" />
            <enum name="right_bottom" value="2" />
            <enum name="left_bottom" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="absProgressBar">
        <attr name="progress" format="integer" />
        <attr name="backgroundColor" format="reference|color" />
        <attr name="textColor" format="reference|color" />
    </declare-styleable>
    <declare-styleable name="progressBar">
        <attr name="startFillColor" format="reference|color" />
        <attr name="middleFillColor" format="reference|color" />
        <attr name="endFillColor" format="reference|color" />
        <attr name="arrowPointColor" format="reference|color" />
        <attr name="arrowPointRadius" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="floatTextProgressBar">
        <attr name="triangleColor" format="reference|color" />
        <attr name="rectColor" format="reference|color" />
        <attr name="fillColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="SampleProgressButton">
        　　
        <attr name="sampleProgressButtonBackgroundColor" format="color" />
        　　
        <attr name="sampleProgressButtonForegroundColor" format="color" />

        <attr name="sampleProgressButtonNormalColor" format="color" />
        　　
        <attr name="sampleProgressButtonTextcolor" format="color" />
        　　
        <attr name="sampleProgressButtonMax" format="integer"/>
        　　
        <attr name="sampleProgressButtonProgress" format="integer"/>
        　　
        <attr name="sampleProgressButtonTextSize" format="dimension"/>
        　　
        <attr name="sampleProgressButtonText" format="string" />
    </declare-styleable>
    <declare-styleable name="RangeSeekBar">
        <attr name="rsb_pointerBackground" format="reference" />
        <attr name="rsb_backgroundColor" format="color" />
        <attr name="rsb_progressColor" format="color" />
        <attr name="rsb_range" format="integer" />
    </declare-styleable>

    <declare-styleable name="TCTouchSeekBar">
        <!--默认刻度图片-->
        <attr name="tsb_dotDefault" format="reference" />
        <!--选中的刻度图片-->
        <attr name="tsb_dotChecked" format="reference" />
    </declare-styleable>


</resources>