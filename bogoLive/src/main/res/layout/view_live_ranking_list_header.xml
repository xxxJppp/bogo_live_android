<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_root"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:background="#f8f8f8"
    android:orientation="vertical"
    android:visibility="visible">

    <TextView
        android:id="@+id/tv_ranking_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="8dp"
        android:drawableLeft="@drawable/ic_live_ranking_list_header_number"
        android:drawablePadding="5dp"
        android:textColor="@color/res_text_gray_l"
        android:textSize="13sp"
        android:visibility="gone" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/ic_live_ranking_list_header_bottom_bg"
            android:scaleType="centerCrop" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="bottom"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="45dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ll_item_second"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical">

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <com.bogokj.library.view.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                            android:id="@+id/civ_head_second"
                            android:layout_width="46dp"
                            android:layout_height="46dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="14dp"
                            app:civ_border_color="@color/white" />

                        <ImageView
                            android:id="@+id/imageView2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="2dp"
                            android:src="@drawable/ic_live_ranking_list_header_second" />

                    </FrameLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_nickname_second"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:maxEms="3"
                            android:maxLines="1"
                            android:textColor="@color/res_text_gray_l"
                            android:textSize="13sp" />

                        <ImageView
                            android:id="@+id/iv_global_male_second"
                            android:layout_width="13dp"
                            android:layout_height="13dp"
                            android:layout_marginLeft="5dp"
                            android:src="@drawable/ic_global_male"
                            android:visibility="gone" />

                        <ImageView
                            android:id="@+id/iv_rank_second"
                            android:layout_width="26dp"
                            android:layout_height="13dp"
                            android:layout_marginLeft="5dp"
                            android:src="@drawable/rank_1"
                            android:visibility="gone" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_num_second"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/tv_ranking_type_second"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="贡献"
                            android:textColor="@color/color_rank_tv_CC45FF"
                            android:textSize="10sp" />

                        <TextView
                            android:id="@+id/tv_num_other_second"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="@color/color_rank_tv_CC45FF"
                            android:textSize="10sp" />

                        <com.bogokj.live.view.LiveStringTicketTextView
                            android:id="@+id/tv_ticket_name_second"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/color_rank_tv_CC45FF"
                            android:textSize="10sp" />
                    </LinearLayout>


                </LinearLayout>

                <TextView
                    android:id="@+id/im_follow_second"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_4"
                    android:background="@drawable/self_side_purple_fifteen_cornor_bac"
                    android:gravity="center"
                    android:paddingLeft="12dp"
                    android:paddingTop="3dp"
                    android:paddingRight="12dp"
                    android:paddingBottom="3dp"
                    android:text="+关注"
                    android:textColor="@color/white"
                    android:visibility="invisible" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ll_item_first"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical">

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <com.bogokj.library.view.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                            android:id="@+id/civ_head_first"
                            android:layout_width="59dp"
                            android:layout_height="59dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="20dp"
                            app:civ_border_color="@color/white" />

                        <ImageView
                            android:layout_width="90dp"
                            android:layout_height="90dp"
                            android:layout_marginLeft="2dp"
                            android:src="@drawable/ic_live_ranking_list_header_first" />

                    </FrameLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_nickname_first"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:maxEms="3"
                            android:maxLines="1"
                            android:textColor="@color/res_text_gray_l"
                            android:textSize="13sp" />

                        <ImageView
                            android:id="@+id/iv_global_male_first"
                            android:layout_width="13dp"
                            android:layout_height="13dp"
                            android:layout_marginLeft="5dp"
                            android:src="@drawable/ic_global_male"
                            android:visibility="gone" />

                        <ImageView
                            android:id="@+id/iv_rank_first"
                            android:layout_width="26dp"
                            android:layout_height="13dp"
                            android:layout_marginLeft="5dp"
                            android:src="@drawable/rank_1"
                            android:visibility="gone" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_num_first"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/tv_ranking_type_first"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="贡献"
                            android:textColor="@color/color_rank_tv_CC45FF"
                            android:textSize="10sp" />

                        <TextView
                            android:id="@+id/tv_num_other_first"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="@color/color_rank_tv_CC45FF"
                            android:textSize="10sp" />

                        <com.bogokj.live.view.LiveStringTicketTextView
                            android:id="@+id/tv_ticket_name_first"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/color_rank_tv_CC45FF"
                            android:textSize="10sp" />
                    </LinearLayout>


                </LinearLayout>

                <TextView
                    android:id="@+id/im_follow_first"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_4"
                    android:background="@drawable/self_side_purple_fifteen_cornor_bac"
                    android:gravity="center"
                    android:paddingLeft="12dp"
                    android:paddingTop="3dp"
                    android:paddingRight="12dp"
                    android:paddingBottom="3dp"
                    android:text="+关注"
                    android:textColor="@color/white"
                    android:visibility="invisible" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="45dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ll_item_third"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical">

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <com.bogokj.library.view.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                            android:id="@+id/civ_head_third"
                            android:layout_width="46dp"
                            android:layout_height="46dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="14dp"
                            app:civ_border_color="@color/white" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="2dp"
                            android:src="@drawable/ic_live_ranking_list_header_third" />

                    </FrameLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_nickname_third"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:maxEms="3"
                            android:maxLines="1"
                            android:textColor="@color/res_text_gray_l"
                            android:textSize="13sp" />

                        <ImageView
                            android:id="@+id/iv_global_male_third"
                            android:layout_width="13dp"
                            android:layout_height="13dp"
                            android:layout_marginLeft="5dp"
                            android:src="@drawable/ic_global_male"
                            android:visibility="gone" />

                        <ImageView
                            android:id="@+id/iv_rank_third"
                            android:layout_width="26dp"
                            android:layout_height="13dp"
                            android:layout_marginLeft="5dp"
                            android:src="@drawable/rank_1"
                            android:visibility="gone" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_num_third"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/tv_ranking_type_third"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="贡献"
                            android:textColor="@color/color_rank_tv_CC45FF"
                            android:textSize="10sp" />

                        <TextView
                            android:id="@+id/tv_num_other_third"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="@color/color_rank_tv_CC45FF"
                            android:textSize="10sp" />

                        <com.bogokj.live.view.LiveStringTicketTextView
                            android:id="@+id/tv_ticket_name_third"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/color_rank_tv_CC45FF"
                            android:textSize="10sp" />
                    </LinearLayout>


                </LinearLayout>

                <TextView
                    android:id="@+id/im_follow_third"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_4"
                    android:background="@drawable/self_side_purple_fifteen_cornor_bac"
                    android:gravity="center"
                    android:paddingLeft="12dp"
                    android:paddingTop="3dp"
                    android:paddingRight="12dp"
                    android:paddingBottom="3dp"
                    android:text="+关注"
                    android:textColor="@color/white"
                    android:visibility="invisible" />

            </LinearLayout>

        </LinearLayout>
    </FrameLayout>
</LinearLayout>